{
	"name": "pl_LocalToAzureTermType",
	"properties": {
		"activities": [
			{
				"name": "LoadFromTermTypeTable",
				"description": "Copy from local term type table to Azure db",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "SqlSource",
						"sqlReaderQuery": "SELECT \n\t\tTermTypeID, TypeName, Description TypeDescription, Available TypeAvailable, SourceID TypeSourceID \nFROM \n\t\tdbo.TermType WHERE SourceID = 2"
					},
					"sink": {
						"type": "SqlSink",
						"writeBatchSize": 10000
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"columnMappings": {
							"TermTypeID": "TermTypeID",
							"TypeName": "TypeName",
							"Description": "TypeDescription",
							"Available": "TypeAvailable",
							"SourceID": "TypeSourceID"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Taxonomy_tst",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlTermTypes2",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "PortThesaurus"
		}
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}